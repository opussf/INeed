<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

	<StatusBar name="INEEDItemBarTemplate" inherits="TextStatusBar" virtual="true">
		<Size x="390" y="12" />
		<BarTexture file="Interface\TargetingFrame\UI-StatusBar" />
		<BarColor r="0.0" g="0.3" b="0.9" />
	</StatusBar>
	<FontString name="INEEDItemBarTextTemplate" inherits="GameFontNormalSmall" virtual="true">
		<Color r="1" g="1" b="1" />
	</FontString>

	<Frame name="INEEDUIFrame" frameStrata="BACKGROUND" toplevel="false" parent="UIParent" enableMouse="true" movable="false">
		<Size x="384" y="512"/>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" relativeTo="UIParent"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture setAllPoints="true">
					<Color r="0.0" g="0.0" b="0.0"/>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="$parent_TitleText" frameStrata="ARTWORK" inherits="GameFontHighlight" text="INEED">
					<Size>
						<AbsDimension x="250" y="14"/>
					</Size>
					<Anchors>
						<Anchor point="TOP" relativePoint="TOP">
							<Offset>
								<AbsDimension x="0" y="-4"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				INEED.UIOnLoad()
			</OnLoad>
		</Scripts>
		<!--
		<Frames>
			<Button name="$parent_Item01" inherits="INEEDItemButtonTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$parent"/>
				</Anchors>
			</Button>

		</Frames>
	-->
	</Frame>



</Ui>