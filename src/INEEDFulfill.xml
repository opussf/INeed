	</Frame>


	<Frame name="INEED_FulfillFrame" parent="UIParent" enableMouse="true" movable="true" hidden="true" resizable="false">
		<Size x="200" y="200"/>
		<Anchors>
			<Anchor point="TOPLEFT" relativeTo="BankPanel" relativePoint="TOPRIGHT" x="35"/>
			<!-- <Anchor point="CENTER"/> -->
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parent_BG" file="Interface\DialogFrame\UI-DialogBox-Background">
					<Anchors>
						<Anchor point="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentClose" inherits="UIPanelCloseButton">
				<Anchors><Anchor point="TOPRIGHT" x="0" y="0"/></Anchors>
				<Scripts>
					<OnClick>self:GetParent():Hide()</OnClick>
				</Scripts>
			</Button>
			<EditBox name="$parentFilter" inherits="InputBoxTemplate">
				<Size x="155" y="20" />
				<Anchors>
					<Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" relativeTo="$parentClose">
						<Offset>
							<AbsDimension x="-10" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>self:SetAutoFocus( false )</OnLoad>
					<!-- <OnTextChanged>Rested.updateFilter()</OnTextChanged> -->
				</Scripts>
			</EditBox>
			<!-- <Button name="$parentItem1" inherits="INEEDItemTemplate">
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentFilter" relativePoint="BOTTOM"/>
					<Anchor point="LEFT" relativeTo="$parent" relativePoint="LEFT"/>
				</Anchors>
			</Button> -->
		</Frames>
		<Scripts>
			<OnShow>INEED.Fulfill_OnShow()</OnShow>
		</Scripts>
	</Frame>
</Ui>


