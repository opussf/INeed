
<!-- CritterEmote_ResponseEdit @VERSION@ -->
<Ui xmlns="http://www.blizzard.com/wow/ui/"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	<Frame name="INEED_Item_template" enableMouse="true" virtual="true">
		<Size x="32" y="32"/>
		<Anchors><Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT"/></Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<Texture name="$parent_Icon">
					<Size x="32" y="32"/>
					<Anchors><Anchor point="LEFT"/></Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>


	<Frame name="INEED_FulfillFrame" parent="UIParent" enableMouse="true" movable="true" hidden="true" resizable="false">
		<Size x="350" y="600"/>
		<Anchors>
			<Anchor point="TOPLEFT" relativeTo="BankPanel" relativePoint="TOPRIGHT" x="35"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parent_BG" file="Interface\DialogFrame\UI-DialogBox-Background">
					<Anchors>
						<Anchor point="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parent_Close" inherits="UIPanelCloseButton">
				<Anchors><Anchor point="TOPRIGHT" x="0" y="0"/></Anchors>
				<Scripts>
					<OnClick>self:GetParent():Hide()</OnClick>
				</Scripts>
			</Button>
			<EditBox name="$parent_Filter" inherits="InputBoxTemplate">
				<Size x="155" y="20" />
				<Anchors>
					<Anchor point="TOPRIGHT" relativePoint="BOTTOMLEFT" relativeTo="$parent_Close">
						<Offset>
							<AbsDimension x="-10" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>self:SetAutoFocus( false )</OnLoad>
					<!-- <OnTextChanged>Rested.updateFilter()</OnTextChanged> -->
				</Scripts>
			</EditBox>
			<Button name="$parent_Item_1" inherits="INEED_Item_template">
			</Button>
		</Frames>
	</Frame>
</Ui>
